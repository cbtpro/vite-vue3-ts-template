{
  "name": "vite-vue3-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode=test",
    "dev:pre": "vite --mode=pre",
    "dev:prod": "vite --mode=prod",
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc && vite build --mode=test",
    "build:pre": "vue-tsc && vite build --mode=pre",
    "build:prod": "vue-tsc && vite build --mode=prod",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy": "npm run build && gh-pages -d dist",
    "dotenv:pull": "npx dotenv-vault@latest pull"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@vuemap/amap-jsapi-loader": "^1.0.4",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.7.7",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.4.3",
    "html2canvas": "^1.4.1",
    "less": "^4.2.2",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.13",
    "@types/amap-js-api": "^1.4.16",
    "@types/node": "^20.17.17",
    "@vitejs/plugin-vue": "^4.6.2",
    "dotenv": "^16.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "msw": "^2.4.9",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.7.3",
    "vite": "^4.5.9",
    "vite-plugin-qrcode": "^0.2.3",
    "vue-tsc": "^2.2.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "engines": {
    "node": ">=14.20.1",
    "npm": ">=6.14.17"
  }
}
