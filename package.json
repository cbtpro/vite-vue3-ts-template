{
  "name": "vite-vue3-ts-template-new",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode=test",
    "dev:pre": "vite --mode=pre",
    "dev:prod": "vite --mode=prod",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:test": "run-p type-check \"build-only {@}\" -- --mode=test",
    "build:pre": "run-p type-check \"build-only {@}\" -- --mode=pre",
    "build:prod": "run-p type-check \"build-only {@}\" -- --mode=prod",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy": "npm run build && gh-pages -d dist",
    "dotenv:pull": "npx dotenv-vault@latest pull",
    "preinstall": "npx only-allow pnpm",
    "gcz": "git-cz",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ types/"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@vuemap/amap-jsapi-loader": "^1.0.4",
    "@vueuse/core": "^10.7.2",
    "ant-design-vue": "~4.2.6",
    "axios": "^1.7.7",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.4.3",
    "html2canvas": "^1.4.1",
    "less": "^4.2.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.13",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.53.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/amap-js-api": "^1.4.16",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.1",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.2.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^13.3.0",
    "msw": "^2.4.9",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-qrcode": "^0.2.3",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "engines": {
    "node": ">=18.20.7",
    "npm": ">=10.8.2"
  }
}
